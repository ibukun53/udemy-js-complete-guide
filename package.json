{
  "name": "udemy-js-complete-guide",
  "version": "1.0.0",
  "scripts": {
    "test": "jest --passWithNoTests",
    "format": "npx prettier  --write '**/*.{json,md,yaml,html,yml}'",
    "format:check": "npx prettier  --check '**/*.{json,md,html,yaml,yml}'",
    "webhint": "npx hint --telemetry=off .",
    "eslint": "npx eslint . --fix",
    "eslint:check": "npx eslint .",
    "stylelint": "npx stylelint \"**/*.{css,scss}\" --fix",
    "stylelint:check": "npx stylelint  \"**/*.{css,scss}\"",
    "lint": "npx eslint '**/*.js' --fix; npx stylelint \"**/*.{css,scss}\" --fix",
    "lint:check": "npx eslint '**/*.js'; npx stylelint  \"**/*.{css,scss}\""
  },
  "description": "A list of coding challenges with solution and test cases.",
  "dependencies": {},
  "devDependencies": {
    "@babel/preset-env": "^7",
    "eslint": "^8",
    "eslint-config-airbnb": "^19",
    "eslint-config-airbnb-base": "^15",
    "eslint-config-prettier": "^8",
    "eslint-plugin-import": "^2",
    "eslint-plugin-prettier": "^4",
    "hint": "^7",
    "jest": "^29",
    "jest-extended": "^3",
    "prettier": "^2",
    "stylelint": "^14",
    "stylelint-config-standard": "^25"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bolah2009/udemy-js-complete-guide.git"
  },
  "keywords": [
    "JavaScript",
    "Jest",
    "Algorithm"
  ],
  "author": "Bola",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bolah2009/udemy-js-complete-guide/issues"
  },
  "homepage": "https://github.com/bolah2009/udemy-js-complete-guide#readme",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "verbose": true
  }
}
